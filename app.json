{
  "expo": {
    "scheme": "acme",
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access camera."
        }
      ]
    ],
    "name": "Parentgrine Falcon",
    "slug": "parentgrine-mobile",
    "experiments": {
      "tsconfigPaths": true
    },
    "android": {
      "package": "com.parentgrine.mobile",
      "permissions": [
        "ACCESS_BACKGROUND_LOCATION",
        "android.permission.CAMERA"
      ]
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "453de83b-8b96-46cf-ba2b-7f2122a163e4"
      }
    }
  }
}
