{
  "expo": {
    "scheme": "acme",
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-barcode-scanner",
        { "cameraPermission": "Allow $(PRODUCT_NAME) to access camera." }
      ]
    ],
    "name": "Parentgrine Falcon",
    "slug": "parentgrine-mobile",
    "experiments": {
      "tsconfigPaths": true
    },
    "android": {
      "package": "com.parentgrine.mobile",
      "permissions": ["ACCESS_BACKGROUND_LOCATION"]
    }
  }
}
